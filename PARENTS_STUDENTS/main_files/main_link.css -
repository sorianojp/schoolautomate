.buttoncontainer{width:90%; height:40px; left:5%; top:3%; position:relative; margin-top:5%; <!-- border:1px gray solid; -->}
.buttonhome{width:48%; height:80%; float:left; border:1px gray solid; color:gray; text-align:center; display:table;
background:#C0733F; color:white; text-shadow: 0 -1px 0 rgba(255, 255, 255, 0.3);}
.buttonhome p{display:table-cell; padding:8% 0 0 0}
.buttonhome:hover{color:white; cursor:pointer;
background: #f5f6f6; /* Old browsers */
/* IE9 SVG, needs conditional override of 'filter' to 'none' */
